{
    "name": "@tiptap-codeless/extension-drag-handle",
    "version": "0.1.1",
    "description": "Drag handle extension with insert functionality for Tiptap, similar to Feishu docs",
    "keywords": [
        "tiptap",
        "editor",
        "drag-handle",
        "insert",
        "react"
    ],
    "author": "namelesserlx (https://github.com/namelesserlx)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/namelesserlx/tiptap-codeless",
        "directory": "packages/extension-drag-handle"
    },
    "homepage": "https://namelesserlx.github.io/tiptap-codeless/drag-handle/",
    "bugs": {
        "url": "https://github.com/namelesserlx/tiptap-codeless/issues"
    },
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "dev": "tsc && vite build --watch",
        "build": "vite build && tsc",
        "type-check": "tsc --noEmit",
        "lint": "eslint src",
        "clean": "rm -rf dist",
        "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
    },
    "dependencies": {
        "@tiptap-codeless/core": "workspace:*",
        "@tiptap/core": "^3.13.0",
        "@tiptap/pm": "^3.13.0",
        "@tiptap/react": "^3.13.0",
        "classnames": "^2.5.1"
    },
    "devDependencies": {
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^4.3.4",
        "react": "^19.2.1",
        "react-dom": "^19.2.1",
        "typescript": "^5.9.3",
        "vite": "^7.2.7"
    },
    "peerDependencies": {
        "@tiptap/core": ">=3.0.0",
        "react": ">=19.0.0",
        "react-dom": ">=19.0.0"
    }
}
